# Vim persistent undo files
*.un~
*.swp
*.swo

# Vim undodir
vim/undodir/*
!vim/undodir/.gitkeep

# Local config
*.local

# zcompile'd files
*.zwc
*.zwc.old

.netrwhist

emacs-plugins/*
!emacs-plugins/.gitkeep

# Ignore Vim bundles
bundle/

# Ignore sessions
sessions/

# Keep these folders...
!vim/backups/.gitkeep
!vim/undodir/.gitkeep
# ...but not their contents
vim/backups/*
vim/undodir/*
# Ignore the binary version because it will be automatically updated and
# reloaded when words are added with `zg`
vim/vim-spell-en.utf-8.add.spl

# Ignore icopy'd files
icopy-*.image

# Ignore old version of vim-plug (created by `:PlugUpgrade`)
plug.vim.old

view/

# Ignore vifm state tracking
vifm/vifminfo.json*

wallpapers/
nvim/plugin/

# systemd auto-generated
systemd/**/*.wants/*

# Ignore hosts file, it contains auth tokens
gh/hosts.yml

# Ignore karabiner backups
karabiner/automatic_backups/*
